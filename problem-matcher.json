{
    "problemMatcher": [
        {
            "owner": "pyright-error",
            "pattern": [
                {
                    "regexp": "^\\s+(\\S+):(\\d+):(\\d+) - ([A-z]+): (.+)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                }
            ]
        }
    ]
}
